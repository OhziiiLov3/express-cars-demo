<%- include('../partials/head') %> 

<h1>All Cars</h1>
<ul>
    <% cars.forEach((car)=>{ %>
        <a href="/cars/<%=car.id%>"> <h1><%= car.make%></h1></a>
        <li>Year: <%= car.year%></li>
        <li>Used: <%= car.isUsed ? "Yes" : "No" %></li>
   <% }) %>
   <a href="/">Go Back</a>
</ul>

